{
  "name": "son-of-atlan",
  "version": "0.0.0",
  "description": "A Simple Dummy Data Fetch Application to show off some UI skills :-P",
  "main": "index.js",
  "repository": "https://github.com/sanmoh-hombal/son-of-atlan",
  "author": "Sanmoh Hombal <sanmoh.hombal@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "vite",
    "test:unit": "vitest --environment jsdom",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.json --composite false",
    "prepare": "husky install"
  },
  "dependencies": {
    "@ant-design/icons-vue": "^6.1.0",
    "ant-design-vue": "^3.2.13",
    "axios": "^1.1.3",
    "vue": "^3.2.41",
    "vue-router": "^4.1.5"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.2.0",
    "@types/jsdom": "^20.0.0",
    "@types/node": "18.8.0",
    "@vitejs/plugin-vue": "^3.1.2",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "@vue/test-utils": "^2.1.0",
    "@vue/tsconfig": "^0.1.3",
    "commitizen": "^4.2.5",
    "cz-emoji-conventional": "^1.0.1",
    "eslint": "^8.25.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-vue": "^9.6.0",
    "husky": "^8.0.0",
    "jsdom": "^20.0.1",
    "lint-staged": "^13.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "typescript": "^4.8.4",
    "vite": "^3.1.8",
    "vitest": "^0.24.3",
    "vue-tsc": "^1.0.8"
  },
  "lint-staged": {
    "*.{vue,js,ts}": "eslint --fix"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-emoji-conventional"
    }
  }
}
